(defpoll battery :interval "15s" "$HOME/.config/eww/eww-bar/scripts/battery --bat")
(defpoll battery_status :interval "1m" "$HOME/.config/eww/eww-bar/scripts/battery --bat-st")

(defwidget battery []
    (box :class "bat_module" :vexpand "false" :hexpand "false" 
        (label :text battery :class "iconbat" )
    )
)

